{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "import/extensions": ["error", "ignorePackages"]
  },
  "env": {
    "node": true,
    "mocha": true,
    "es2021": true

  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "plugin:import/recommended",
    "airbnb-base"
  ],
  "plugins": ["import"]
}
